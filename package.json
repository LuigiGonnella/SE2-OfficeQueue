  "description": "Repository of SE2 course at Politecnico di Torino",
  "main": "index.js",
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "debug-unix": "nodemon -r tsconfig-paths/register --exec 'node --inspect-brk -r ts-node/register' ./src/index.ts",
    "debug-win": "nodemon -r tsconfig-paths/register --exec \"node --inspect-brk -r ts-node/register\" ./src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LuigiGonnella/SE2-OfficeQueue.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LuigiGonnella/SE2-OfficeQueue/issues"
  },
  "homepage": "https://github.com/LuigiGonnella/SE2-OfficeQueue#readme",
  "dependencies": {
    "bootstrap": "^5.3.8",
    "config": "^4.1.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-openapi-validator": "^5.6.0",
    "js-yaml": "^4.1.0",
    "reflect-metadata": "^0.1.14",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.27",
    "winston": "^3.18.3",
    "yamljs": "^0.3.0"
  },
